%{
#include "y.tab.h"  // Include the header file generated by YACC
%}

%%
[0-9]+         { yylval.num = atoi(yytext); return NUMBER; }
"+"            { return PLUS; }
"-"            { return MINUS; }
"*"            { return MUL; }
"/"            { return DIV; }
"("            { return LPAREN; }
")"            { return RPAREN; }
[ \t\n]        { /* Ignore whitespace */ }
.              { return yytext[0]; }

%%

int yywrap() {
    return 1;  // Return 1 when the input is finished
}

